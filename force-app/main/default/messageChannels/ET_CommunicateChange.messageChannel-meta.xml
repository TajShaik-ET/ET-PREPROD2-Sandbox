<?xml version="1.0" encoding="UTF-8"?>
<LightningMessageChannel xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This Lightning Message Channel is to communicate change in Vehicle Quantity and No
        of Drivers</description>
    <isExposed>true</isExposed>
    <lightningMessageFields>
        <description>This is data parameter</description>
        <fieldName>changeInVehicleQuantity</fieldName>
    </lightningMessageFields>
    <lightningMessageFields>
        <description>This is another data parameter</description>
        <fieldName>changeInDriverQuantity</fieldName>
    </lightningMessageFields>
    <masterLabel>ET_CommunicateChange</masterLabel>
</LightningMessageChannel>
