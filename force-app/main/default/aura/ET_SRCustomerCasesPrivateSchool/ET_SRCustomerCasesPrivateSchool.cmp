                                                                isTrue="{!case.RecordType.Name == 'Request for the awareness sessions'}">
                                                                <lightning:button variant="neutral"
                                                                    label="{!v.clApprove}" value="{!case.Id}"
                                                                    title="{!v.clApprove}" aura:id="Approve"
                                                                    onclick="{!c.openConfirmModal}" />

                                                            </aura:if>
                                                        </aura:if>
                                                      

                                                         
