                                            <tr class="slds-line-height_reset">
                                                  <th class="" scope="col">
                                                        <div class="slds-truncate" title="Actions">
                                                            <a onclick="{!c.handleSortSRCase}" Id="caseAction"
                                                                style="color: #8f8888;">
                                                                {!v.clfilePreview}
                                                            </a>
                                                        </div>
                                                    </th>
                                                
                                                <th class="" scope="col">
                                                    <div class="slds-truncate" title="Actions">
                                                        <a onclick="{!c.handleSortSRCase}" Id="caseAction"
                                                            style="color: #8f8888;">
                                                            {!v.clAction}
                                                        </a>
                                                    </div>
                                                </th>
                                                <th class="" scope="col">
                                                    <div class="slds-truncate" title="Students">
                                                        <a onclick="{!c.handleSortSRCase}" Id="caseStudent"
                                                            style="color: #8f8888;">
                                                            {!v.clStudent}
                                                        </a>
                                                    </div>
                                                </th>
                                               <!-- <th class="" scope="col">
                                                    <div class="slds-truncate" title="Assigned Vehicles">
                                                        <a onclick="{!c.handleSortSRCase}" Id="caseVec"
                                                            style="color: #8f8888;">
                                                            {!v.clAssignedVehicles}
                                                        </a>
                                                    </div>
                                                </th>-->
                                                <th class="" scope="col">
                                                    <div class="slds-truncate" title="Assigned Resources">
                                                        <a onclick="{!c.handleSortSRCase}" Id="caseRes"
                                                            style="color: #8f8888;">
                                                            {!v.clAssignedResources}
                                                        </a>
                                                    </div>
                                                </th>
                                                <th class="" scope="col">
                                                    <div class="slds-truncate" title="Status">
                                                        <a onclick="{!c.handleSortSRCase}" Id="preAct"
                                                            style="color: #8f8888;">
                                                            {!v.clSchoolName}
                                                        </a>
                                                    </div>
                                                </th>
                                                <th class="" scope="col">
                                                    <div class="slds-truncate" title="Status">
                                                        <a onclick="{!c.handleSortSRCase}" Id="preAct"
