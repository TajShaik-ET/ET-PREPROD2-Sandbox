                                                                         (v.isMOEExtraCurricularActivities &amp;&amp; case.Status_Category__c=='Pending Hemam for Inclusive Education services')||
                                                                         (v.isInclusiveDepartment &amp;&amp; case.Status_Category__c=='Pending with Inclusive Education Department in ESE' &amp;&amp; case.Sub_Status__c=='Approved by Hemam for Inclusive Education services')||
                                                                         (v.isPersonnelManagementDepartment &amp;&amp; case.Status_Category__c=='Pending with MOE - Personnel Affairs')||
