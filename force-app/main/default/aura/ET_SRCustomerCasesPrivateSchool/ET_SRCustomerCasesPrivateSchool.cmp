    <aura:if isTrue="{!v.newCase}">
        <aura:if isTrue="{!v.accEmirates == 'Abu Dhabi'}">
            <!--###### MODAL BOX Start######-->
            <c:CaseB2BPopupMOE aura:id="CaseB2BPopupMOE" newCase="{!v.newCase}"
                               userParentProfileWrap="{!v.userParentProfileWrap}" />
            <div class="slds-backdrop slds-backdrop_open"></div>
            <aura:set attribute="else">
                   <c:CaseB2BPopupMOENonAbuDhabi aura:id="CaseB2BPopupMOE" newCase="{!v.newCase}"
                               userParentProfileWrap="{!v.userParentProfileWrap}" />
            <div class="slds-backdrop slds-backdrop_open"></div>
            </aura:set>
        </aura:if>
    </aura:if>
    <aura:if isTrue="{!(v.isSchoolActivitiesDepartment)}">
        <aura:if isTrue="{!v.newCase}">
            <!--###### MODAL BOX Start######-->
            <c:ActivityDepartmentCasePopup aura:id="CaseB2BPopupMOE" newCase="{!v.newCase}"
                userParentProfileWrap="{!v.userParentProfileWrap}" />
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>
    </aura:if>


    <div class="menudiv">
        <c:MenuItemsList />
    </div>
    <div class="casesupmaindiv">
        <aura:if isTrue="{!v.customerSupportDashboard}">
            <div class="fivediv">
                <article class="slds-card" style="text-align:center;">
                    <a href="" id="escalatedcase" onclick="{!c.showAllCasesAction}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.total_img}" />
                                        {!v.allRequestsCount}

                                    </div>

                                    <h3 class="form-title">{!v.clAllSpecialRequests} </h3>
                                </div>
                            </div>
                        </div>
                    </a>
                    <!--      <a href="" id="escalatedcase" onclick="{!c.handleApprovalCasesClick}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.newcase_img}"/>
                                        {!v.newCases}
                                       
                                    </div>
                                    <h3 class="form-title">{!v.clNew}</h3>
                                </div>
                            </div>
                        </div>
                    </a>-->
                    <a href="" id="escalatedcase" onclick="{!c.handleApprovalCasesClick}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.pandingcase_img}" />
                                        {!v.escalatedCases}
                                    </div>
                                    <h3 class="form-title">{!v.clPendingApproval}</h3>
                                </div>
                            </div>
                        </div>
                    </a>

                    <a href="" id="holdcase" onclick="{!c.handleOnHoldCasesClick}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.rejectcase_img}" />
                                        {!v.onHoldCases}
                                    </div>
                                    <h3 class="form-title">{!v.clRejected}</h3>
                                </div>
                            </div>
                        </div>
                    </a>
                    <a href="" id="closedcase" onclick="{!c.handleClosedCasesClick}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.closecase_img}" />
                                        {!v.closedCases}
                                    </div>
                                    <h3 class="form-title">{!v.clClosed}</h3>
                                </div>
                            </div>
                        </div>
                    </a>

                    <a href="" id="cancelcase" onclick="{!c.handleCancelCasesClick}">
                        <div class="slds-col">
                            <div class="form-box-row2" id="step-1">
                                <div class="rightsectiondiv">
                                    <div class="numberSize">
                                        <img src="{!$Resource.Cancelld_img}" />
                                        {!v.cancelCases}
                                    </div>
                                    <h3 class="form-title">{!v.clCancelled}</h3>
                                </div>
                            </div>
                        </div>
                    </a>



                </article>
            </div>
        </aura:if>
        <aura:if isTrue="{!not(v.isSpecialEducationDepartment)}">
            <aura:if isTrue="{!not(v.isSchoolServicesDepartment)}">
                <aura:if isTrue="{!not(v.isSchoolActivitiesDepartment)}">
                    <aura:if isTrue="{!not(v.isPersonnelManagementDepartment)}">
                        <aura:if isTrue="{!not(v.isMainSchoolContact)}">
                            <aura:if isTrue="{!not(v.isMOEExtraCurricularActivities)}">
                                <aura:if isTrue="{!not(v.isInclusiveDepartment)}">
                                    <aura:if isTrue="{!not(v.isESEsupportCenter)}">
                                    <div
                                        class="{!v.clLang =='ar' ? 'btncreatesectionar slds-p-right_small slds-p-bottom_medium btncreatesection' : 'slds-p-right_small slds-p-bottom_medium btncreatesection'}">
                                        <lightning:button
                                            class="{!v.clLang =='ar' ? 'emmergencyButtonar emmergencyButton' : 'emmergencyButton'}"
                                            label="{!v.clCreateRequest}" title="{!v.clCreateRequest}"
                                            onclick="{!c.createCasePopupMoE}" />

                                        <lightning:button
                                            class="{!v.clLang =='ar' ? 'emmergencyButtonar emmergencyButton' : 'emmergencyButton'}"
                                            label="{!v.clAllSpecialRequests}" title="{!v.clAllSpecialRequests}"
                                            onclick="{!c.showAllCasesAction}" />
                                       <lightning:button
                                            class="{!v.clLang =='ar' ? 'emmergencyButtonar emmergencyButton' : 'emmergencyButton'}"
                                            label="{!v.clDownload}" title="{!v.clDownload}"
                                            onclick="{!c.downloadReportpopup}" />
                                    </div>
                                   </aura:if>
                                </aura:if>
                            </aura:if>
                        </aura:if>
                    </aura:if>
                </aura:if>
            </aura:if>
        </aura:if>
        <aura:if isTrue="{!(v.isSchoolActivitiesDepartment)}">
            <lightning:button class="{!v.clLang =='ar' ? 'emmergencyButtonar emmergencyButton' : 'emmergencyButton'}"
                label="{!v.clCreateRequest}" title="{!v.clCreateRequest}" onclick="{!c.createAcitvityCase}" />
        </aura:if>
        <aura:if isTrue="{!(v.userEmail=='malha.albreiki@ese.gov.ae')}">
            <lightning:button
                              class="{!v.clLang =='ar' ? 'emmergencyButtonar emmergencyButton' : 'emmergencyButton'}"
